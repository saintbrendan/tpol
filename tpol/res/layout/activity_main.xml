<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/hello_world" />

    <GridLayout
        android:id="@+id/gridLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:columnCount="3" >

        <TextView
            android:id="@+id/textView2"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:layout_row="0"
            android:text="CDMA RSSI " />

        <TextView
            android:id="@+id/textView3"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:layout_row="0"
            android:text="0" />

        <TextView
            android:id="@+id/textView4"
            android:layout_column="2"
            android:layout_gravity="right|top"
            android:layout_row="0"
            android:text=" dBm" />

        <TextView
            android:id="@+id/textView5"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:layout_row="1"
            android:text="CDMA Ec/Io " />

        <TextView
            android:id="@+id/textView6"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:layout_row="1"
            android:text="0" />

        <TextView
            android:id="@+id/textView7"
            android:layout_column="2"
            android:layout_gravity="right|top"
            android:layout_row="1"
            android:text=" dB*10" />

        <TextView
            android:id="@+id/textView8"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:layout_row="2"
            android:text="EVDO RSSI " />

        <TextView
            android:id="@+id/textView9"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:layout_row="2"
            android:text="0" />

        <TextView
            android:id="@+id/textView10"
            android:layout_column="2"
            android:layout_gravity="right|top"
            android:layout_row="2"
            android:text=" dBm" />

        <TextView
            android:id="@+id/textView11"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:layout_row="3"
            android:text="EVDO Ec/Io " />

        <TextView
            android:id="@+id/textView12"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:layout_row="3"
            android:text="0" />

        <TextView
            android:id="@+id/textView13"
            android:layout_column="2"
            android:layout_gravity="right|top"
            android:layout_row="3"
            android:text=" dB*10" />

        <TextView
            android:id="@+id/textView14"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:layout_row="4"
            android:text="EVDO S/N " />

        <TextView
            android:id="@+id/textView15"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:layout_row="4"
            android:text="0" />

        <TextView
            android:id="@+id/textView16"
            android:layout_column="2"
            android:layout_gravity="right|top"
            android:layout_row="4"
            android:text=" [0-8]" />

        <TextView
            android:id="@+id/textView17"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_gravity="left|fill_vertical"
            android:layout_row="7"
            android:text="GSM bit error rate " />

        <TextView
            android:id="@+id/textView18"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:layout_row="7"
            android:text="0" />

        <TextView
            android:id="@+id/textView19"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_gravity="right|fill_vertical"
            android:layout_row="7"
            android:text=" [0-7,99]" />

        <TextView
            android:id="@+id/textView20"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:layout_row="8"
            android:text="GSM Signal Strength " />

        <TextView
            android:id="@+id/textView21"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:layout_row="8"
            android:text="0" />

        <TextView
            android:id="@+id/textView22"
            android:layout_column="2"
            android:layout_gravity="right|top"
            android:layout_row="8"
            android:text=" [0-31,99]" />

        <TextView
            android:id="@+id/textView23"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:layout_row="6"
            android:text="is GSM?" />

        <TextView
            android:id="@+id/textView24"
            android:layout_column="1"
            android:layout_gravity="left|top"
            android:layout_row="6"
            android:text="false" />
    </GridLayout>
    
</RelativeLayout>